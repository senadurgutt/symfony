use App\Controller\Admin\MemberController;

{% extends 'adminbase.html.twig' %}

{% block title %}Kullanıcı Listesi{% endblock %}

{% block body %}

    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Düzenleme
                <small><a href="{{ path('member_create')  }}">Kullanıcı ekle </a></small>

            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li><a href="#">Examples</a></li>
                <li class="active">bıdıbıdıbıd page</li>
            </ol>
        </section>

        <section class="content">

            <div class="box box-primary">
                <div class="box-header">
                    <h3 class="box-title">Quick Example</h3>
                </div><!-- /.box-header -->
                <!-- form start -->
                <form role="form" method="post">
                    <div class="box-body">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Name</label>
                            <input type="name" name="member[name]" class="form-control" id="_name" placeholder="name">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Surname</label>
                            <input type="surname" name="member[surname]" class="form-control" id="_surname" placeholder="Surname">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Email address</label>
                            <input type="email" name="member[email]" class="form-control" id="_email" placeholder="Enter email">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Password</label>
                            <input type="password" name="member[password]" class="form-control" id="_password" placeholder="Password">
                        </div>
                    </div><!-- /.box-body -->

                    <div class="box-footer">
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                </form>
            </div>
            {#
              {{ form_start(form) }}#}
{#            {{ form_widget(form) }}#}
{#            {{ form_end(form) }}#}

        </section>

        <!-- Main content -->
        <section class="content">

            <div class="box">
                <div class="box-footer clearfix">
                    <ul class="pagination pagination-sm no-margin pull-right">
                        <li><a href="#">&laquo;</a></li>
                        <li><a href="#">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a href="#">&raquo;</a></li>
                    </ul>
                </div>
            </div><!-- /.box -->

        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->

{% endblock %}
