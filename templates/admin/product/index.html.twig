{% extends 'adminbase.html.twig' %}
{% block title %}Ürün Listesi{% endblock %}
{% block body %}
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>Ürün Listesi
                <small><a href="{{ path('product_new') }}">Ürün ekle</a></small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li><a href="#">Examples</a></li>
                <li class="active">Ürün Listesi</li>
            </ol>
        </section>
        <!-- Main content -->
        <section class="content">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Bordered Table</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                    <table class="table table-bordered">
                        <thead>
                        <tr>
                            <th>Id</th>
                            <th>Title</th>
                            <th>Description</th>
                            <th>Member Name</th>
                            <th>Image</th>
                            <th>Category Name</th>
                            <th>Actions</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for product in products %}
                            <tr>
                                <td>{{ product.id }}</td>
                                <td>{{ product.title }}</td>
                                <td>{{ product.description }}</td>
                                <td>{{ product.member.name }} {{ product.member.surname }}</td>
                                <td>
                                    <a href="javascript:openWin('{{ path('product_imgedit', {'id': product.id}) }}', '', 'toolbar=no,location=no,directories=0,status=no,top=100,left=200,menubar=no,scrollbars=no,resizable=no,width=600,height=500');">
                                        {% if product.image is null %}
                                            Add Image
                                        {% else %}
                                            <img src="{{ asset('img/' ~ product.image) }}" height="40">
                                        {% endif %}
                                    </a>
                                </td>
                                <td>{{ product.category.title }}</td>
                                <td>
                                    <a href="{{ path('product_show', {'id': product.id}) }}" class="btn btn-primary">Göster</a>
                                    <a href="{{ path('product_edit', {'id': product.id}) }}" class="btn btn-primary">Düzenle</a>
                                </td>
                            </tr>
                        {% else %}
                            <tr>
                                <td colspan="7">No records found</td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>

                    <a href="{{ path('product_new') }}">Create new</a>
                </div><!-- /.box-body -->
                <div class="box-footer clearfix">
                    <ul class="pagination pagination-sm no-margin pull-right">
                        <li><a href="#">&laquo;</a></li>
                        <li><a href="#">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a href="#">&raquo;</a></li>
                    </ul>
                </div>
            </div><!-- /.box -->
        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->

    <script>
        function openWin(url, name, specs) {window.open(url, name, specs);
        }
    </script>
{% endblock %}
